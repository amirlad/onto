<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/bimprev#"
    xml:base="https://w3id.org/bimprev"
     xmlns:ifc="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="https://w3id.org/bimprev"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/bimprev#reduce -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#reduce">
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#SafetyMeasure"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#Risk"/>
        <rdfs:label>reduce</rdfs:label>
        <rdfs:comment>Safety measure that reduces a specific risk.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#accomplish -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#accomplish">
        <owl:inverseOf rdf:resource="https://w3id.org/bimprev#isAccomplishedBy"/>
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcActor"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/bimprev#IfcProcess"/>
                    <rdf:Description rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcTask"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>accomplish</rdfs:label>
        <rdfs:comment>Links an actor or task to the process it accomplishes.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#hasActivity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#hasActivity">
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcActor"/>
        <rdfs:range rdf:resource="https://w3id.org/bimprev#IfcProcess"/>
        <rdfs:label>hasActivity</rdfs:label>
        <rdfs:comment>Associates an entity with an IFC process activity.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#hasTask -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#hasTask">
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#IfcProcess"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcTask"/>
        <rdfs:label>hasTask</rdfs:label>
        <rdfs:comment>Indicates a task that belongs to or is required by a process.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#isLocatedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#isLocatedIn">
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#IfcProcess"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcBuildingElement"/>
        <rdfs:label>isLocatedIn</rdfs:label>
        <rdfs:comment>States the hosting building element for a process.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#isPresenting -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#isPresenting">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/bimprev#IfcProcess"/>
                    <rdf:Description rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcTask"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#Risk"/>
        <rdfs:label>isPresenting</rdfs:label>
        <rdfs:comment>Indicates that a process or task presents a given risk.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#need -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#need">
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcTask"/>
        <rdfs:range rdf:resource="https://w3id.org/bimprev#SafetyMeasure"/>
        <rdfs:label>need</rdfs:label>
        <rdfs:comment>Declares that a task requires a safety measure.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#isHorizontalHoleOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#isHorizontalHoleOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bimprev#isHoleOf"/>
        <owl:inverseOf rdf:resource="https://w3id.org/bimprev#hasHorizontalHole"/>
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcSlab"/>
        <rdfs:label>isHorizontalHoleOf</rdfs:label>
        <rdfs:comment>Relates a hopper that forms a horizontal hole within a slab.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#isVerticalHoleOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#isVerticalHoleOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bimprev#isHoleOf"/>
        <owl:inverseOf rdf:resource="https://w3id.org/bimprev#hasVerticalHole"/>
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcSlab"/>
        <rdfs:label>isVerticalHoleOf</rdfs:label>
        <rdfs:comment>Relates a hopper that forms a vertical hole within a parent element.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#relatedElements -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#relatedElements">
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcRoof"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcRailing"/>
        <rdfs:label>relatedElements</rdfs:label>
        <rdfs:comment>Links a roof to a railing element related to it.</rdfs:comment>
        <owl:inverseOf rdf:resource="https://w3id.org/bimprev#relatingStructure"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#relatingStructure -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#relatingStructure">
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcRailing"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcRoof"/>
        <rdfs:label>relatingStructure</rdfs:label>
        <rdfs:comment>Associates a railing with the roof structure it relates to.</rdfs:comment>
        <owl:inverseOf rdf:resource="https://w3id.org/bimprev#relatedElements"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#hasHole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#hasHole">
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcBuildingElement"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcOpeningElement"/>
        <owl:inverseOf rdf:resource="https://w3id.org/bimprev#isHoleOf"/>
        <rdfs:label>hasHole</rdfs:label>
        <rdfs:comment>States that a building element contains an opening element.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#hasHorizontalHole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#hasHorizontalHole">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bimprev#hasHole"/>
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcSlab"/>
        <rdfs:range rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:label>hasHorizontalHole</rdfs:label>
        <rdfs:comment>Indicates that a slab includes a horizontal hopper opening.</rdfs:comment>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/bimprev#hasVerticalHole"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#hasType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#hasType">
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcOpeningElement"/>
        <rdfs:label>hasType</rdfs:label>
        <rdfs:comment>Assigns an IFC opening element type to a hopper.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#hasVerticalHole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#hasVerticalHole">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bimprev#hasHole"/>
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcSlab"/>
        <rdfs:range rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:label>hasVerticalHole</rdfs:label>
        <rdfs:comment>Indicates that a slab includes a vertical hopper opening.</rdfs:comment>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/bimprev#hasHorizontalHole"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#isAccomplishedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#isAccomplishedBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/bimprev#IfcProcess"/>
                    <rdf:Description rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcTask"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcActor"/>
        <rdfs:label>isAccomplishedBy</rdfs:label>
        <rdfs:comment>Inverse relation showing which task or process accomplishes a target.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#isEquippedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#isEquippedBy">
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcElement"/>
        <rdfs:label>isEquippedBy</rdfs:label>
        <rdfs:comment>Declares the IFC element that equips a hopper.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#isHoleOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#isHoleOf">
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcOpeningElement"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcBuildingElement"/>
        <rdfs:label>isHoleOf</rdfs:label>
        <rdfs:comment>Inverse relation indicating the host building element of an opening.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bimprev#requires -->

    <owl:ObjectProperty rdf:about="https://w3id.org/bimprev#requires">
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#IfcProcess"/>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcElement"/>
        <rdfs:label>requires</rdfs:label>
        <rdfs:comment>Specifies the IFC element needed by a process.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/bimprev#hasDiameter -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/bimprev#hasDiameter">
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>hasDiameter</rdfs:label>
        <rdfs:comment>Stores a diameter measurement.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/bimprev#hasHeight -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/bimprev#hasHeight">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcRailing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>hasHeight</rdfs:label>
        <rdfs:comment>Stores a height value, typically for railings.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/bimprev#hasLength -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/bimprev#hasLength">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcOpeningElement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>hasLength</rdfs:label>
        <rdfs:comment>Stores a length measurement for an opening element.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/bimprev#hasSpacement -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/bimprev#hasSpacement">
        <rdfs:domain rdf:resource="https://w3id.org/bimprev#Barring"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>hasSpacement</rdfs:label>
        <rdfs:comment>Stores spacing between bars in a barring element.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/bimprev#hasWidth -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/bimprev#hasWidth">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcOpeningElement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>hasWidth</rdfs:label>
        <rdfs:comment>Stores a width measurement for an opening element.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/bimprev#IfcProcess -->

    <owl:Class rdf:about="https://w3id.org/bimprev#IfcProcess">
        <rdfs:subClassOf rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcObject"/>
        <rdfs:label>IfcProcess</rdfs:label>
        <rdfs:comment>Represents a work process or activity in a construction or building lifecycle context.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#SafetyMeasure -->

    <owl:Class rdf:about="https://w3id.org/bimprev#SafetyMeasure">
        <rdfs:label>SafetyMeasure</rdfs:label>
        <rdfs:comment>Represents preventive actions or equipment used to reduce risk and ensure safety during construction processes.</rdfs:comment>
    </owl:Class>
    


    <!-- https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcActor -->

    <owl:Class rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcActor">
        <rdfs:subClassOf rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcObject"/>
        <rdfs:label>IfcActor</rdfs:label>
        <rdfs:comment>Represents a person, organization, or other entity that performs activities in a building project.</rdfs:comment>
    </owl:Class>
    


    <!-- https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcObject -->

    <owl:Class rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4/FINAL/OWL#IfcObject">
        <rdfs:label>IfcObject</rdfs:label>
        <rdfs:comment>Base IFC class for objects that can have properties, relationships, and other attributes in the IFC model.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#Barring -->

    <owl:Class rdf:about="https://w3id.org/bimprev#Barring">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bimprev#ProtectiveEquipement"/>
        <rdfs:subClassOf rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcBuildingElement"/>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bimprev#BarringCircularOpening"/>
            <rdf:Description rdf:about="https://w3id.org/bimprev#BarringSlotOpening"/>
            <rdf:Description rdf:about="https://w3id.org/bimprev#BarringSquareOpening"/>
        </owl:disjointUnionOf>
        <rdfs:label>Barring</rdfs:label>
        <rdfs:comment>A protective barrier consisting of bars or grids covering an opening to prevent falls or unauthorized access.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#BarringCircularOpening -->

    <owl:Class rdf:about="https://w3id.org/bimprev#BarringCircularOpening">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bimprev#Barring"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/bimprev#hasSpacement"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">24</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>BarringCircularOpening</rdfs:label>
        <rdfs:comment>Protective barring designed specifically for circular openings with maximum bar spacing of 24 units.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#BarringSlotOpening -->

    <owl:Class rdf:about="https://w3id.org/bimprev#BarringSlotOpening">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bimprev#Barring"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/bimprev#hasSpacement"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">18</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>BarringSlotOpening</rdfs:label>
        <rdfs:comment>Protective barring for slot-shaped openings with maximum bar spacing of 18 units.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#BarringSquareOpening -->

    <owl:Class rdf:about="https://w3id.org/bimprev#BarringSquareOpening">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bimprev#Barring"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/bimprev#hasSpacement"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">24</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>BarringSquareOpening</rdfs:label>
        <rdfs:comment>Protective barring for square or rectangular openings with maximum bar spacing of 24 units.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#Hatch -->

    <owl:Class rdf:about="https://w3id.org/bimprev#Hatch">
        <rdfs:subClassOf rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcBuildingElement"/>
        <rdfs:label>Hatch</rdfs:label>
        <rdfs:comment>A movable cover or door providing access to an opening in a floor, roof, or ceiling.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#Hinge -->

    <owl:Class rdf:about="https://w3id.org/bimprev#Hinge">
        <rdfs:subClassOf rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcBuildingElement"/>
        <rdfs:label>Hinge</rdfs:label>
        <rdfs:comment>A mechanical bearing connecting two parts to allow rotation, typically used on doors or hatches.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#Hopper -->

    <owl:Class rdf:about="https://w3id.org/bimprev#Hopper">
        <rdfs:subClassOf rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcOpeningElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/bimprev#isEquippedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/bimprev#Barring"/>
                            <rdf:Description rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2/OWL#IfcRailing"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bimprev#HopperSquareHuman"/>
            <rdf:Description rdf:about="https://w3id.org/bimprev#HopperSquareObject"/>
        </owl:disjointUnionOf>
        <rdfs:label>Hopper</rdfs:label>
        <rdfs:comment>An opening in a floor or slab that may be equipped with protective equipment such as barring or railings.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#HopperSquareHuman -->

    <owl:Class rdf:about="https://w3id.org/bimprev#HopperSquareHuman">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:label>HopperSquareHuman</rdfs:label>
        <rdfs:comment>A square or rectangular hopper opening designed for human passage or access.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/bimprev#HopperSquareObject -->

    <owl:Class rdf:about="https://w3id.org/bimprev#HopperSquareObject">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bimprev#Hopper"/>
        <rdfs:label>HopperSquareObject</rdfs:label>
        <rdfs:comment>A square or rectangular hopper opening designed for passing objects or materials.</rdfs:comment>
    </owl:Class>
    


